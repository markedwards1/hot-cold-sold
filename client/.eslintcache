utation } from '@apollo/client';\n\nimport { ADD_CLIENT } from '../../utils/mutations';\n// import { ALL_CLIENT} from '../../utils/queries';\n\nimport Auth from '../../utils/auth';\n\nconst AddClientForm = () => {\n    const[name, setName] = useState('');\n    const[phone, setPhone] = useState('');\n    const[email, setEmail] = useState('');\n    const[product, setProduct] = useState('');\n    const[note, setNote] = useState('');\n    const[status, setStatus] = useState('');\n\n\n\n  const [addClient, { error }] = useMutation(ADD_CLIENT);\n\n  const handleFormSubmit = async (event) => {\n    event.preventDefault();\n\n    try {\n      const { data } = await addClient({\n        variables: {\n          \n          email,\n          name,\n          note,\n          phone,\n          product,\n          status,\n         \n        },\n      });\n\n      setName('');\n      setEmail(''); \n      setNote('');\n      setPhone('');\n      setProduct('');\n      setStatus('');\n      \n    } catch (err) {\n      console.error(err);\n    }\n  };\n\n  const handleChange = (event) => {\n    const { name, value } = event.target;\n\n    if (name === 'name') {\n      setName(value);\n      } else if (name === 'email') {\n        setEmail(value);\n        }  if (name === 'note') {\n          setNote(value);\n          }  if (name === 'phone') {\n            setPhone(value);\n            }  if (name === 'product') {\n              setProduct(value);\n              }  if (name === 'status') {\n                setStatus(value);\n                } \n  };\n\n  return (\n    <div>\n      <h3>Add New Client</h3>\n\n      {Auth.loggedIn() ? (\n        <>\n \n          <form\n            className=\"flex-row justify-center justify-space-between-md align-center\"\n            onSubmit={handleFormSubmit}\n          >\n            <div className=\"col-12 col-lg-9\">\n              <textarea\n                name=\"name\"\n                placeholder=\"Add name\"\n                value={name}\n                className=\"form-input w-100\"\n                style={{ lineHeight: '1.5', resize: 'vertical' }}\n                onChange={handleChange}\n              ></textarea>\n            </div>\n            <div className=\"col-12 col-lg-9\">\n              <textarea\n                name=\"email\"\n                placeholder=\"Add email\"\n                value={email}\n                className=\"form-input w-100\"\n                style={{ lineHeight: '1.5', resize: 'vertical' }}\n                onChange={handleChange}\n              ></textarea>\n            </div>\n            <div className=\"col-12 col-lg-9\">\n              <textarea\n                name=\"note\"\n                placeholder=\"Add note\"\n                value={note}\n                className=\"form-input w-100\"\n                style={{ lineHeight: '1.5', resize: 'vertical' }}\n                onChange={handleChange}\n              ></textarea>\n            </div>\n            <div className=\"col-12 col-lg-9\">\n              <textarea\n                name=\"phone\"\n                placeholder=\"Add phone\"\n                value={phone}\n                className=\"form-input w-100\"\n                style={{ lineHeight: '1.5', resize: 'vertical' }}\n                onChange={handleChange}\n              ></textarea>\n            </div>\n            <div className=\"col-12 col-lg-9\">\n              <textarea\n                name=\"product\"\n                placeholder=\"Add product\"\n                value={product}\n                className=\"form-input w-100\"\n                style={{ lineHeight: '1.5', resize: 'vertical' }}\n                onChange={handleChange}\n              ></textarea>\n            </div>\n            <div className=\"col-12 col-lg-9\">\n              <textarea\n                name=\"status\"\n                placeholder=\"Add status\"\n                value={status}\n                className=\"form-input w-100\"\n                style={{ lineHeight: '1.5', resize: 'vertical' }}\n                onChange={handleChange}\n              ></textarea>\n            </div>\n\n            <div className=\"col-12 col-lg-3\">\n              <button className=\"btn btn-primary btn-block py-3\" type=\"submit\">\n                Add Client\n              </button>\n            </div>\n            {error && (\n              <div className=\"col-12 my-3 bg-danger text-white p-3\">\n                {error.message}\n              </div>\n            )}\n          </form>\n        </>\n      ) : (\n        <p>\n          You need to be logged in to share your thoughts. Please{' '}\n          <Link to=\"/login\">login</Link> or <Link to=\"/signup\">signup.</Link>\n        </p>\n      )}\n    </div>\n  );\n};\n\nexport default AddClientForm;\n\n\n\n\n\n\n\n\n\n\n\n\n","/Users/markedwards/Desktop/homework/hot-cold-sold/client/src/components/ClientList/index.js",[],"/Users/markedwards/Desktop/homework/hot-cold-sold/client/src/pages/Addclient.js",[],"/Users/markedwards/Desktop/homework/hot-cold-sold/client/src/pages/OpenClient.js",[],"/Users/markedwards/Desktop/homework/hot-cold-sold/client/src/components/CommunicationList/index.js",[],"/Users/markedwards/Desktop/homework/hot-cold-sold/client/src/components/AddComs/index.js",["71"],{"ruleId":"72","replacedBy":"73"},{"ruleId":"74","replacedBy":"75"},{"ruleId":"76","severity":1,"message":"77","line":26,"column":15,"nodeType":"78","messageId":"79","endLine":26,"endColumn":19},{"ruleId":"76","severity":1,"message":"77","line":19,"column":15,"nodeType":"78","messageId":"79","endLine":19,"endColumn":19},"no-native-reassign",["80"],"no-negated-in-lhs",["81"],"no-unused-vars","'data' is assigned a value but never used.","Identifier","unusedVar","no-global-assign","no-unsafe-negation"]